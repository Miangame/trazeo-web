{% extends 'TrazeoFrontBundle:Panel:base.html.twig' %}

		{% block body_content %}
		
			<div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">
                    {{ "Panel.title" | trans  }}
                    </h1>
                </div><!-- /.col-lg-12 -->
		{% endblock %}
		
{% block body -%}
    <h1>Children</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nick</th>
                <td>{{ entity.nick }}</td>
            </tr>
            <tr>
                <th>Datebirth</th>
                <td>{{ entity.dateBirth|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Visibility</th>
                <td>{{ entity.visibility }}</td>
            </tr>
            <tr>
                <th>Sex</th>
                <td>{{ entity.sex }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('children') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('children_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
		