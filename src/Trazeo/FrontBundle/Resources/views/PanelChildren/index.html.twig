{% extends 'TrazeoFrontBundle:Panel:base.html.twig' %}

		{% block body_content %}
		
			<div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">
                    {{ "Panel.title" | trans  }}
                    </h1>
                </div><!-- /.col-lg-12 -->
                
                {% for entity in entities %}
            <tr>
               
                <td>{{ entity.nick }}</td>
                <td>{% if entity.dateBirth %}{{ entity.dateBirth|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.visibility }}</td>
                <td>{{ entity.sex }}</td>
                <td>
                
                </td>
            </tr>
        {% endfor %}
		{% endblock %}
		
