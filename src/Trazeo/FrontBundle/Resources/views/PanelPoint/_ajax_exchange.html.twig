<script type="text/javascript">
	$(".exchange-code").click(function()
	{
		$.ajax({
 			url: "http://localhost/trazeo-web/web/app_dev.php/api/exchange/code",
  			type: "POST",
  			data: { code : $("#exchangecode").val()}
		}).done(function(textResponse){
			if(textResponse=='ok')alert(":D");
			$("#exchangecode").val("");
		});
	});
</script>
