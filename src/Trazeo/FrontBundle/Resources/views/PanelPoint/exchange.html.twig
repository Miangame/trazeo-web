{%set i =0%}
	{%set j =0%}
{%for ofert in oferts %}
	{%if ofert.complete==1 %}
		{%if j==0%}<div class="row col-md-12 col-xs-12 col-sm-12 col-lg-12" style="margin-top:10px">{%endif%}
		{% include 'TrazeoFrontBundle:PanelPoint:_row_catalogItem.html.twig' with {
		'ofert': ofert, 'file': files[i]
		} %}
		{%set j=j+1%}
		{%if j==3%}</div>{%set j=0%}{%endif%}
	{%endif%}
	{%set i =i+1%}
{%endfor%}