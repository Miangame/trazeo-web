<!-- START resumeGroups -->
 <div class="resumeGroups alert" id="groups">
 	{% if (groupsMember|length > 0) %}
 		<h3>Tus grupos</h3>
 		{% for groupMember in groupsMember %}
 			{% include 'TrazeoFrontBundle:PanelGroups:_rowGroup.html.twig' with {'groupMember': groupMember}  %}
 		{% endfor %}
 	{% endif %}
 	
 	<h3>
 		<div class="pull-left">
 			{{ 'list_groups_available'|trans|raw }}
 		</div>
 		<div class="pull-right">
			<select class="form-control change-city" name="city">
				<option value="all"> - {{ "City.all"|trans }} - </option>
				{% for c in cities %}
				<option {% if city == c.id %} selected="selected" {% endif %}value="{{ c.nameUtf8 }}">{{ c.nameUtf8 }}</option>
				{% endfor %}
			</select>
 		</div>
 	</h3>
 	<div style="clear:both"></div>	
 	{% for restGroup in restGroups %}
 		{% if restGroup.Visibility  != 2 %}
 			{% include 'TrazeoFrontBundle:PanelGroups:_rowGroup.html.twig' with {'groupMember': restGroup, 'nois': 1}  %}
 		{% endif %}
 	{% endfor %}
 </div>