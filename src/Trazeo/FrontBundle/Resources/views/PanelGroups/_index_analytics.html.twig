<script type="text/javascript">
	var analytics_user=analytics.identify('{{ app.user.id }}',{email: '{{ app.user.email }}', username: '{{ app.user }}'});
	//Analytics invite group button 	
	$(".group-join").click(function (event){
		var group_id=$(event.currentTarget).attr('group.name');
		analytics.track('click.groups.join',{group: group_id}); 
	});	
</script>