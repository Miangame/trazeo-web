<div class="col-md-12 resumeRoutes" id="routes">
{% if routes|length > 0 %}
	<div class="row">
		<div class="pull-left">
			<h1>
				<span class="alert"><i class="fa fa-compass"></i> {{ 'Route.mine'|trans }}</span>
				</span>
			</h1>
		</div>
		<div class="pull-right"><br/>
			<a class="btn btn-success" href="{{ path('panel_route') }}">
				<i class="fa fa-list-ul"></i>&nbsp; {{ 'Route.list'|trans }}
			</a>		
			<a class="btn btn-success" href="{{ path('panel_route_new') }}">
				<i class="fa fa-plus-circle"></i>&nbsp; {{ 'Route.new.title'|trans }}
			</a>
		</div>
	</div>
		
    {% for route in routes %}
      	{% include 'TrazeoFrontBundle:PanelRoutes:_rowRoute.html.twig' with {'route':route} %}
    {% endfor %}
    
{% else %}
	<div class="row alert-success">
		<div class="col-md-6">
			<i class="fa fa-compass fa-5x pull-left"></i>
			<h3>
			{{ 'Route.empty'|trans }}
			&nbsp;&nbsp;
			</h3>
		</div>
		<div class="pull-right"><br/>
			<a class="btn btn-warning" href="{{ path('panel_route') }}">
				<i class="fa fa-list-ul"></i>&nbsp; {{ 'Route.list'|trans }}
			</a>&nbsp;		
			<a class="btn btn-warning" href="{{ path('panel_route_new') }}">
				<i class="fa fa-hand-o-right"></i> {{ 'Route.new.title'|trans }}
			</a>
		</div>
	</div>
	{#
		<i class="fa fa-compass fa-5x pull-left alert-warning"></i>
