{% extends 'TrazeoFrontBundle:Panel:base.html.twig' %}

		{% block body_content %}
		
			<div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">
                    {{ "Panel.title" | trans  }}
                    </h1>
                </div><!-- /.col-lg-12 -->
		{% endblock %}
		
% block body -%}
    <h1>Routes</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('panel_routes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('panel_routes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
		