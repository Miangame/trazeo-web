<script type="text/javascript">
	var analytics_user=analytics.identify('{{ app.user.id }}',{email: '{{ app.user.email }}'});
	//Analytics login user button 	
	$(".ride-action").click(function (event){
		analytics.track('click.ride.action',{ride: $(event.currentTarget).attr('ride')}) 
	});
	//Analytics new child prev
	$("#new_child").click(function (event){
		analytics.track('click.child.prev.new') 
	});
</script>