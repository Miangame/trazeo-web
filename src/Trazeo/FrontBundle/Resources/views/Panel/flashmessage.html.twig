{% for type, flashMessages in app.session.flashbag.all() %}
	{% for flashMessage in flashMessages %}
	<script>
		$.msgGrowl({
			'type': '{{ type }}',
			'title': '{{ flashMessage }}',
			'text': '{{ flashMessage }}'
		});
		$('.msgGrowl-container').css('z-index','10000');
	</script>
	{% endfor %}
{% endfor %}