{
  "name" : "Trazeo",
  "description" : "Trazeo Project",
  "minimum-stability" : "dev",
  "autoload" : {
    "psr-4": {
      "": "src/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "repositories" : [{
    "type" : "package",
    "package" : {
      "name" : "jquery/jquery",
      "version" : "1.10.2",
      "dist" : {
        "url" : "http://code.jquery.com/jquery-1.10.2.js",
        "type" : "file"
      }
    }
  }
  ],
  "require" : {
    "php": ">=5.3.3",
    "symfony/symfony": "2.8.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/data-fixtures": "^1.2",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "twig/extensions": "~1.0",
    "symfony/assetic-bundle": "2.8.0",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "sensio/distribution-bundle": "~4.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "sensio/generator-bundle": "~3.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "kriswallsmith/assetic": "1.*",
    "sonata-project/doctrine-extensions": "1.*",
    "sonata-project/easy-extends-bundle" : "2.1.*",
    "sonata-project/cache-bundle": "dev-master#70460807b1c735632e1185b3f9cbfeb4489388f6",
    "sonata-project/jquery-bundle": "1.8.*",
    "sonata-project/exporter": "1.1.0",
    "sonata-project/google-authenticator": "1.*",
    "sonata-project/user-bundle": "2.2.*",
    "sonata-project/block-bundle": "2.2.*",
    "sonata-project/core-bundle" : "~2.2@dev",
    "twitter/bootstrap": "2.3.*",
    "craue/formflow-bundle": "2.1.x-dev",
    "sonata-project/doctrine-orm-admin-bundle": "2.2.*",
    "knplabs/knp-components": "dev-master#ffafb21dc1665c733fdb3aabdcb04ea65d0bd7f3",
    "knplabs/knp-paginator-bundle": "~2.3",
    "knplabs/knp-menu-bundle": "1.1.*",
    "stof/doctrine-extensions-bundle": "dev-master#2ff8473e33e224af87dc00d48a552a35a75115af",
    "a2lix/translation-form-bundle" : "2.x-dev",
    "sopinet/bootstrap-extend-bundle": "dev-master",
    "friendsofsymfony/rest-bundle": "1.*",
    "friendsofsymfony/user-bundle" : "1.3.*",
    "jms/serializer-bundle": "dev-master",
    "jms/security-extra-bundle": "dev-master",
    "jms/di-extra-bundle": "dev-master",
    "raulfraile/ladybug-bundle": "v0.7",
    "twbs/bootstrap": "3.0.*",
    "knplabs/doctrine-behaviors": "~1.1",
    "egeloen/ckeditor-bundle": "~2.0",
    "jjs/geonames-bundle" : "dev-master",
    "jms/translation-bundle" : "dev-master",
    "elao/error-notifier-bundle" : "dev-master",
    "hwi/oauth-bundle" : "0.4.*@dev",
    "sopinet/userbundle" : "dev-master",
    "jmikola/auto-login-bundle" : "~1.0",
    "friendsofsymfony/comment-bundle" : "dev-master",
    "sopinet/timeline-bundle" : "dev-master",
    "sopinet/simple-point-bundle": "dev-master",
    "sopinet/flash-messages-bundle" : "dev-master",
    "jms/serializer": "dev-master",
    "lunetics/locale-bundle": "2.2.*",
    "sopinet/upload-magic-bundle" : "dev-master",
    "phpunit/phpunit-selenium": ">=1.2",
    "hipaway-travel/mandrill-bundle": "dev-master",
    "sopinet/autologin-bundle": "dev-master",
    "ob/highcharts-bundle": "~1.2@dev",
    "nelmio/api-doc-bundle": "2.x",
    "richsage/rms-push-notifications-bundle": "dev-master",
    "oldsound/rabbitmq-bundle": "1.*",
    "slot/mandrill-bundle": "dev-master",
    "sopinet/template-amoeba-bundle": "0.0.1",
    "sopinet/template-landing-bundle": "0.0.1",
    "sopinet/template-sbadmin2-bundle": "dev-master"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
